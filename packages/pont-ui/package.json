{
  "name": "pont-ui",
  "version": "0.0.1",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "dev": "vite",
    "watch": "tsc -w",
    "less": "find src -name '*.less'  -or -name '*.css' -exec rsync -R '{}' lib \\; && cp -R lib/src/* lib/ && rm -rf lib/src",
    "build": "tsc && npm run less",
    "preview": "vite preview"
  },
  "files": [
    "lib",
    "dist"
  ],
  "dependencies": {
    "pont-core": "*",
    "pont-spec": "*",
    "@blueprintjs/core": "^4.1.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@vitejs/plugin-react": "^1.0.7",
    "typescript": "^4.5.4",
    "vite": "^2.9.0"
  }
}