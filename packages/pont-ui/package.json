{
  "name": "pont-ui",
  "version": "0.0.1",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "dev": "vite",
    "watch": "tsc -w",
    "less": "find src -name '*.less'  -or -name '*.css' -exec rsync -R '{}' lib \\; && cp -R lib/src/* lib/ && rm -rf lib/src",
    "build": "tsc && npm run less",
    "preview": "vite preview"
  },
  "files": [
    "lib",
    "dist"
  ],
  "peerDependencies": {
    "react": "*",
    "react-dom": "*"
  },
  "dependencies": {
    "@blueprintjs/core": "^4.1.2",
    "pont-spec": "file:../pont-spec"
  },
  "devDependencies": {
    "@types/react": "^18.0.8",
    "@types/react-dom": "^18.0.3",
    "@vitejs/plugin-react": "^1.3.2",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "typescript": "^4.6.4",
    "vite": "^2.9.7"
  }
}
