{
  "name": "pont-ui",
  "version": "0.3.1",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "start": "vite",
    "watch": "tsc -w",
    "copy-files": "cd src && copyfiles -V \"./**/*.less\" ../lib && copyfiles -V \"./**/*.css\" ../lib && cd .. && cp -r src/style/ lib/style/",
    "build": "tsc --build && npm run copy-files",
    "preview": "vite preview"
  },
  "files": [
    "lib",
    "dist",
    "package.json",
    "*.md"
  ],
  "peerDependencies": {
    "lodash": "*",
    "react": "*",
    "react-dom": "*"
  },
  "dependencies": {
    "@alicloud/console-components": "^1.5.7",
    "@uiw/react-markdown-preview": "^4.0.16",
    "@vscode/codicons": "^0.0.30",
    "bootstrap": "^5.1.3",
    "classnames": "^2.3.1",
    "pont-spec": "^0.3.1",
    "pont-spec-diff": "^0.3.1",
    "react-diff-viewer": "^3.1.1",
    "react-input-autosize": "^3.0.0",
    "react-window": "^1.8.7",
    "unstated-next": "^1.1.0"
  },
  "devDependencies": {
    "@types/lodash": "*",
    "@types/node": "*",
    "@types/react": "^16",
    "@types/react-dom": "^16",
    "@vitejs/plugin-react": "^1.3.2",
    "copyfiles": "^2.4.1",
    "less": "^4.1.2",
    "lodash": "*",
    "react": "^16",
    "react-dom": "^16",
    "typescript": "^4.6.4",
    "vite": "^2.9.7"
  }
}
