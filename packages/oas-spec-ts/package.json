{
  "name": "oas-spec-ts",
  "version": "0.0.1",
  "description": "OpenAPI Spec OAS TS Typescript",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "pretest": "npm run build",
    "dev": "tsc -p ./scripts/tsconfig.json -w",
