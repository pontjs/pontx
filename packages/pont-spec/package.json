{
  "name": "pont-spec",
  "version": "0.3.2",
  "description": "pont spec",
  "module": "es6/index.js",
  "main": "lib/index.js",
  "typings": "es6/index.d.ts",
  "scripts": {
    "watch": "tsc --build --watch",
    "clean": "rimraf lib && rimraf tsconfig.tsbuildinfo",
