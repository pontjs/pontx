{
  "name": "vscode-pont2",
  "displayName": "pont2",
  "description": "",
  "version": "0.0.1",
  "private": true,
  "publisher": "jasonHzq",
  "license": "MIT",
  "engines": {
    "vscode": "^1.47.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension",
  "contributes": {
    "jsonValidation": [
      {
        "fileMatch": "pont-config.json",
        "url": "./configSchema.json"
      }
    ],
    "commands": [
      {
        "command": "pont.switchOrigin",
        "title": "switchOrigin pont current origin"
      },
      {
        "command": "pont.findInterface",
        "title": "find pont interface"
      },
      {
        "command": "pont.openPontUI",
        "title": "open pont tab"
      },
      {
        "command": "pont.pont.regenerate",
        "title": "regenerate local spec sdk code"
      },
      {
        "command": "pont.restart",
        "title": "restart vscode-pont"
      }
    ],
    "keybindings": [
      {
        "command": "pont.findInterface",
        "key": "cmd+ctrl+p",
        "mac": "cmd+ctrl+p"
      },
      {
        "command": "pont.refreshMocks",
        "key": "cmd+ctrl+m",
        "mac": "cmd+ctrl+m"
      }
    ],
    "menus": {
      "editor/context": []
    }
  },
  "scripts": {
    "watch": "tsc --build --watch",
    "clean": "rimraf oub && rimraf tsconfig.tsbuildinfo",
    "build": "npm run clean && tsc --build",
    "test": "npm run build && node ../../node_modules/vscode/bin/test",
    "media": "cd media && vite --mode browser",
    "build:media": "cd media && vite build",
    "preview": "cd media && vite build --watch"
  },
  "dependencies": {
    "fs-extra": "^7.0.1",
    "mockjs": "^1.0.1-beta3",
    "pont-manager": "*",
    "pont-spec": "*",
    "pont-spec-diff": "*"
  },
  "icon": "logo.png",
  "repository": {
    "url": "git@github.com:pont2/pont.git",
    "type": "git"
  },
  "devDependencies": {
    "@types/fs-extra": "^5.1.0",
    "@types/lodash": "^4.14.182",
    "@types/react": "^18.0.8",
    "@types/react-dom": "^18.0.3",
    "@types/vscode": "^1.47.0",
    "@types/vscode-webview": "^1.57.0",
    "@vitejs/plugin-react": "^1.3.2",
    "@vscode/codicons": "^0.0.30",
    "pont-ui": "*",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "typescript": "^4.6.4",
    "vite": "^2.9.7",
    "vsce": "^2.7.0"
  }
}
